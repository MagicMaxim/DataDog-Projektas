<title>Users</title>
{% block stylesheets %}
    <link  rel="stylesheet"  href="css\RegCss.css">
{% endblock %}
{% block body %}
    <div class="form">
        <form class="register-form">
                <h1>All users</h1>

    <table>
        <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>

        </tr>
        </thead>
        <tbody>
        {% for user in users %}
            <tr>
                <td>{{ user.username }}</td>
                <td>{{ user.email }}</td>
                <td>
                    {{ include('user/_delete_form.html.twig') }}

                </td>

            </tr>
        {% else %}
            <tr>
                <td colspan="8">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <div class="navigation text-center">
        {{ knp_pagination_render(users) }}
    </div>
            </form>

</div>
{% endblock %}